   "source": [
    "nn = len(tlist)\n",
    "ylist = []\n",
    "yylist = []\n",
    "yyylist = []\n",
    "erlist = []\n",
    "errlist = []\n",
    "errrlist = []\n",
    "for i in range (0, nn):\n",
    "    truey = realy(tlist[i])\n",
    "    ylist.append(truey)\n",
    "    erlist.append(abs(truey - wlist[i]))\n",
    "    trueyy = realy(ttlist[i])\n",
    "    yylist.append(trueyy)\n",
    "    errlist.append(abs(trueyy - wwlist[i]))\n",
    "    trueyyy = realy(tttlist[i])\n",
    "    yyylist.append(trueyyy)\n",
    "    errrlist.append(abs(trueyyy - wwwlist[i]))"
   ]
