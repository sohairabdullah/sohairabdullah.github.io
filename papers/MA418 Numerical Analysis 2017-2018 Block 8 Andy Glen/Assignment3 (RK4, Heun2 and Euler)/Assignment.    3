   "source": [
    "def f(x, y):\n",
    "    # return 2\n",
    "    # return x * y\n",
    "    # return y - x ** 2 + 1\n",
    "    return cos(2*x) + sin(3*x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 119,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Euler (a, b, n, A):\n",
    "    h = (b - a)/n\n",
    "    t = a\n",
    "    w = A\n",
    "    tlist = [a]\n",
    "    wlist = [A]\n",
    "    for i in range (1, n + 1):\n",
    "        k1 = f(t, w)\n",
    "        w = w + h * f(t, w)\n",
    "        t = a + i * h\n",
    "        wlist.append(w)\n",
    "        tlist.append(t)\n",
    "        #print('t = %.3f'%(t), 'w = ', w)\n",
    "    return tlist, wlist"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "metadata": {},
   "outputs": [],
   "source": [
    "def heun2 (a, b, n, A):\n",
    "    h = (b - a)/n\n",
    "    t = a\n",
    "    w = A\n",
    "    tlist = [a]\n",
    "    wlist = [A]\n",
    "    for i in range (1, n + 1):\n",
    "        k1 = f(t, w)\n",
    "        k2 = f(t + h, w + k1 * h)\n",
    "        slope = (k1 + k2)/2\n",
    "        w = w + h * slope\n",
    "        t = a + i * h\n",
    "        wlist.append(w)\n",
    "        tlist.append(t)\n",
    "        #print('t = %.3f'%(t), 'w = ', w)\n",
    "    return tlist, wlist"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 121,
   "metadata": {},
   "outputs": [],
   "source": [
    "def RK4 (a, b, n, A):\n",
    "    h = (b - a)/n\n",
    "    t = a\n",
    "    w = A\n",
    "    tlist = [a]\n",
    "    wlist = [A]\n",
    "    for i in range (1, n + 1):\n",
    "        K1 = h * f(t, w)\n",
    "        K2 = h * f(t + h/2, w + K1/2)\n",
    "        K3 = h * f(t + h/2, w + K2/2)\n",
    "        K4 = h * f(t + h, w + K3)\n",
    "        w = w + (K1 + 2*K2 + 2*K3 + K4)/6\n",
    "        t = a + i * h\n",
    "        wlist.append(w)\n",
    "        tlist.append(t)\n",
    "        #print('t = %.3f'%(t), 'w = ', w)\n",
    "    return tlist, wlist"
   ]
  },
