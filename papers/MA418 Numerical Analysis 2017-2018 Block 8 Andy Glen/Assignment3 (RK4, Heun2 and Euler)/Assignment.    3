    "def totalError(list1): #n=5000\n",
    "    error = 0\n",
    "    for i in range(0, len(list1)):\n",
    "        error = list1[i]\n",
    "        \n",
    "    return error\n",
    "\n",
    "EulerError = totalError(erlist)\n",
    "Heun2Error = totalError(errlist)\n",
    "RK4Error = totalError(errrlist)\n",
    "\n",
    "print (\"The total error given by Euler's method is \" + str(EulerError))\n",
